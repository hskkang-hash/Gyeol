{
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5003
    },
    "hub": {
      "port": 4405
    },
    "ui": {
      "port": 4005
    },
    "logging": {
      "port": 4505
    }
  },
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
